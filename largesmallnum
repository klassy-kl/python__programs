even_arr=[]
odd_arr=[]
def largesmallnum(arr):
    n=len(arr)
    for i in range(1,n):
        if i%2==0:
            even_arr.append(arr[i])
        else:
            odd_arr.append(arr[i])
    return even_arr,odd_arr        

arr=[3,2,1,7,5,4]
even_arr.sort()
odd_arr.sort()
largesmallnum(arr)
print(odd_arr[-2])
